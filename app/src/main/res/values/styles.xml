<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--Primary Text Bold-->
    <style name="PrimaryTextBold" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:fontFamily">@font/font_bold</item>
        <item name="font">@font/font_bold</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="PrimaryTextRegular" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:fontFamily">@font/font_regular</item>
        <item name="font">@font/font_regular</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="PrimaryTextLight" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:fontFamily">@font/font_light</item>
        <item name="font">@font/font_light</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <!--Circle Image-->
    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="ShapeAppearanceOverlay.App.CornerRadius" parent="">
        <item name="cornerSizeBottomRight">5dp</item>
        <item name="cornerSizeBottomLeft">5dp</item>
        <item name="cornerSizeTopLeft">5dp</item>
        <item name="cornerSizeTopRight">5dp</item>
    </style>



</resources>